@model List<User>;
@{
}
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Student Table</h4>
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                Id
                            </th>
                            <th>
                                Name
                            </th>
                            <th>
                                Phones
                            </th>
                            <th>
                                Email
                            </th>
                            <th colspan="2">
                                Actions
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.Id
                                </td>
                                <td>
                                    @item.Name
                                </td>
                                <td>
                                    @item.Phone
                                </td>
                                <td>
                                    @item.Email
                                </td>
                                <td colspan="2">
                                    <a asp-action="" type="button" class="btn btn-sm btn-outline-primary">Edit</a>
                                    <a asp-action="" type="button" class="btn btn-sm btn-outline-danger">Delete</a>
                                </td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>